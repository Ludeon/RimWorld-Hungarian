  <ArmorRating_Heat.description>Védelem nagy hő ellen, pl. tűz.</ArmorRating_Heat.description>
  <!-- EN: Armor - Heat (armor stat) -->
  <ArmorRating_Heat.labelForFullStatList>TODO</ArmorRating_Heat.labelForFullStatList>
  
